@model IEnumerable<Fixture02.Models.Jigitem>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/FixtureMaster.cshtml";
}

    <h2>采购入库申请</h2>

<p>
    @Html.ActionLink("新增申请", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        @*工夹具Code对应的Name*@
        <th>
            @Html.DisplayNameFor(model => model.Jig.Name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.SeqID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BillNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BackNote)
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Code)
    </td>

    @*工夹具Code对应的Name*@
    <th>
        @Html.DisplayFor(modelItem => item.Jig.Name)
    </th>

    <td>
        @Html.DisplayFor(modelItem => item.SeqID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BillNo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RegDate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Location)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.State)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AddDate)
    </td>
    <th>
        @Html.DisplayFor(modelItem => item.BackNote)
    </th>

    <td>
        @Html.ActionLink("编辑", "Edit", new { id = item.ItemID }) |
        @Html.ActionLink("明细", "Details", new { id = item.ItemID }) |
        @Html.ActionLink("删除", "Delete", new { id = item.ItemID })
    </td>
</tr>
}
</table>
